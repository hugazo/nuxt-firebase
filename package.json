{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "concurrently --names 'nuxt,emulator,functions' 'nuxt dev' 'pnpm emulators' 'pnpm dev:functions'",
    "emulators": "firebase emulators:start --only=auth,functions",
    "dev:functions": "cd functions && pnpm run dev",
    "generate": "nuxt generate",
    "preview": "pnpm build && firebase emulators:start",
    "postinstall": "nuxt prepare"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vueuse/core": "^10.7.2",
    "@vueuse/nuxt": "^10.7.2",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "firebase-admin": "^11.11.1",
    "firebase-functions": "^4.6.0",
    "firebase-functions-test": "^3.1.1",
    "nuxt": "^3.10.1",
    "vite-plugin-vuetify": "^1.0.2",
    "vue": "^3.4.15",
    "vue-eslint-parser": "^9.4.2",
    "vue-router": "^4.2.5",
    "vuetify": "^3.5.2"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@pinia/nuxt": "^0.5.1",
    "firebase": "^10.8.0",
    "nuxt-vuefire": "^1.0.1",
    "pinia": "^2.1.7",
    "vuefire": "^3.1.22"
  }
}
