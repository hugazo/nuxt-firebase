{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "concurrently --names 'nuxt,emulator,functions' 'nuxt dev' 'pnpm emulators' 'pnpm dev:functions'",
    "emulators": "firebase emulators:start --only=auth,functions",
    "dev:functions": "cd functions && pnpm run dev",
    "generate": "nuxt generate",
    "preview": "pnpm build && firebase emulators:start",
    "postinstall": "nuxt prepare"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vueuse/core": "^10.6.1",
    "@vueuse/nuxt": "^10.6.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-import": "^2.25.2",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^4.5.0",
    "firebase-functions-test": "^3.1.0",
    "nuxt": "^3.8.2",
    "vite-plugin-vuetify": "^1.0.2",
    "vue": "^3.3.8",
    "vue-eslint-parser": "^9.3.2",
    "vue-router": "^4.2.5",
    "vuetify": "^3.4.4"
  },
  "dependencies": {
    "@mdi/font": "^7.3.67",
    "firebase": "^10.7.1",
    "nuxt-vuefire": "^1.0.0",
    "vuefire": "^3.1.19"
  }
}
